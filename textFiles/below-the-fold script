  <!-- Below-the-fold scripts -->
    <script type="module">
      import { createDataModel } from "./js/model.js" // might skip the model.js file adnd import directly
      // import { renderDoR } from "./js/presenter/directorOfResearch.js"

      const initialiseDataModel = (
        error,
        rawClubDetails,
        rawGameweekStats,
      ) => {
        if (error) {
          console.error(`There is an error importing the data`, error)
          return
        }

        const model = createDataModel(
           rawClubDetails,
           rawGameweekStats,
        )

        console.log("The model is:", model)

        // renderDoR(model)

      const data = {
        clubDetails: "../data/clubDetails.csv",
        gameweeksStats: "../data/gameweeksStats.csv",
      }

      // Queue and import the data using the LoadData function
      d3.queue()
        .defer(d3.csv, data.clubDetails)
        .defer(d3.csv, data.gameweeksStats)
        .await(initialseDataModel)
		
   </script>